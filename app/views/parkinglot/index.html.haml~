:javascript
	function spot(id, stat, type, occ){
		console.log("SPOTting");
		console.log("Width: " + "#{@parkinglot.width}");
		document.getElementById("spotID").textContent=id;
		document.getElementById("status").textContent=stat;
		document.getElementById("spotType").textContent=type;
		document.getElementById("occupying").textContent=occ;
		event.preventDefault();
	}

%table#parkingspots
	%thead
	%tbody
		- lot = @parkinglot
		- @parkingspots.each_slice(lot.width) do |row|
			%tr
				- row.each do |spot|
					%td= link_to spot.status, "", :onclick=>"spot(\"#{spot.id}\", \"#{spot.status}\", \"#{spot.spot_type}\", \"#{spot.occupying}\")"

%div#div1
	%p#spotID
	%p#status
	%p#spotType
	%p#occupying			


= link_to 'Back', root_path

